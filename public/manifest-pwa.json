{
  "name": "Bible Expository Notes",
  "short_name": "BibleNotes",
  "id": "/?source=pwa",
  "start_url": "/?source=pwa",
  "scope": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone"],
  "orientation": "any",
  "background_color": "#000000",
  "theme_color": "#f8d77f",
  "description": "A comprehensive tool for managing, organizing, and searching bible expository notes with advanced features for pastors and church members.",
  "categories": ["productivity", "education", "lifestyle"],
  "lang": "en-US",
  "dir": "ltr",
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "prefer_related_applications": false,
  "icons": [
    {
      "src": "manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "manifest-icon-512.maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "logo.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "logo.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "apple-icon-180.png",
      "sizes": "180x180",
      "type": "image/png",
      "purpose": "any"
    }
  ],  "screenshots": [
    {
      "src": "Bible Chapters.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Bible Chapters View"
    },
    {
      "src": "Dashboard.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Dashboard Overview"
    },
    {
      "src": "Edit Page.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Edit Sermon Notes"
    },
    {
      "src": "Presentation.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Presentation Mode"
    },
    {
      "src": "Scripture Overlay.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Scripture Overlay Feature"
    },
    {
      "src": "Analytics.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Analytics Dashboard"
    },
    {
      "src": "Themes and Topics.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Themes and Topics Organization"
    }
  ],
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "text/plain": [".txt", ".md"],
        "application/json": [".json"]
      }
    }
  ],
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "web+sermonotes",
      "url": "/?protocol=%s"
    }
  ],
  "share_target": {
    "action": "/",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "shortcuts": [
    {
      "name": "New Sermon Note",
      "short_name": "New Note",
      "description": "Create a new sermon note",
      "url": "/new-sermon?source=shortcut",
      "icons": [
        {
          "src": "manifest-icon-192.maskable.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Search Notes",
      "short_name": "Search",
      "description": "Search through your sermon notes",
      "url": "/scripture-search?source=shortcut",
      "icons": [
        {
          "src": "manifest-icon-192.maskable.png",
          "sizes": "192x192"
        }
      ]
    }
  ]
}